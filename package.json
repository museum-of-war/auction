{
  "name": "@museum-of-war/auction",
  "version": "5.0.3",
  "license": "UNLICENSED",
  "keywords": [
    "ethereum"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "verify-staging": "npx hardhat verify --network staging",
    "deploy-staging": "hardhat run --network staging scripts/deploy.ts",
    "clean": "npx hardhat clean && rm -rf dist",
    "release": "yarn build && yarn version && yarn package && yarn publish --non-interactive --access public ./dist",
    "package": "json-mask 'name,version,license,description,repository,main,types,dependencies,keywords' package.json > dist/package.json && cp readme.md dist/readme.md",
    "build": "yarn clean && yarn compile && tsc -p dist.tsconfig.json",
    "eslint": "eslint '**/*.{js,ts}'",
    "solhint": "solhint 'contracts/**/*.sol'",
    "prettier": "prettier '**/*.{json,sol,md}' --check",
    "eslint-fix": "eslint '**/*.{js,ts}' --fix",
    "solhint-fix": "solhint 'contracts/**/*.sol' --fix",
    "prettier-fix": "prettier '**/*.{json,sol,md}' --write",
    "fix": "yarn prettier-fix && yarn solhint-fix && yarn eslint-fix",
    "lint": "yarn prettier && yarn solhint && yarn eslint",
    "typechain": "hardhat typechain",
    "qa": "yarn lint && yarn test",
    "compile": "hardhat compile",
    "test": "hardhat test"
  },
  "devDependencies": {
    "@ethersproject/providers": "^5.6.2",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/contracts": "^4.5.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.2.0",
    "chai-ethers": "^0.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.9.3",
    "hardhat-deploy": "^0.11.4",
    "hardhat-gas-reporter": "^1.0.8",
    "json-mask": "^1.0.4",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.16",
    "ts-node": "^10.1.0",
    "typechain": "^5.1.2",
    "typescript": "^4.5.2"
  }
}
